// Runtime: 16 ms, faster than 78.20% of C++ online submissions for Majority
// Element II. Memory Usage: 10.8 MB, less than 13.89% of C++ online submissions
// for Majority Element II.

class Solution {
public:
  vector<int> majorityElement(vector<int> &nums) {
    int candidate_1 = 0, candidate_2 = 0;
    int cnt1 = 0, cnt2 = 0;
    for (const int n : nums) {
      if (n == candidate_1)
        cnt1++;
      else if (n == candidate_2)
        cnt2++;
      else if (cnt1 == 0) {
        candidate_1 = n;
        cnt1 = 1;
      } else if (cnt2 == 0) {
        candidate_2 = n;
        cnt2 = 1;
      } else {
        cnt1--;
        cnt2--;
      }
    }

    cnt1 = cnt2 = 0;
    for (const int n : nums) {
      if (n == candidate_1)
        cnt1++;
      else if (n == candidate_2)
        cnt2++;
    }

    vector<int> ans;
    if (nums.size() / 3 < cnt1)
      ans.push_back(candidate_1);
    if (nums.size() / 3 < cnt2)
      ans.push_back(candidate_2);
    return ans;
  }
};