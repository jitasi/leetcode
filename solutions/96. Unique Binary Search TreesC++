class Solution {
public:
    int numTrees(int n) {
      vector<int> dp(n+1);
      dp[0] = 1;
      for (int i = 1; i <= n; i++) {
        dp[i] = 0;
        for (int j = 0; j < i; j++) {
          dp[i] += dp[j] * dp[i - j - 1];
        }
      }  
      return dp[n];
    }
};

// 0 => 1
// 1 => 1
// 2 => 2
// 3 => 5
// .  1 (0, 2) -> 2
// .  2 (1, 1) -> 1  
// .  3 (2, 0) -> 2
// 4 => 14
// .  1 (0, 3) -> 5
// .  2 (1, 2) -> 1 * 2 = 2
// .  3 (2, 1) -> 1 * 2 = 2
// .  4 (3, 0) -> 5
