class Solution {
public:
    vector<int> findDisappearedNumbers(vector<int>& nums) {
      for (int i = 0; i < nums.size(); i++) nums[i]--;
      
      for (int i = 0; i < nums.size(); i++) {
        if (nums[i] == i) continue;
        
        int j = nums[i];
        nums[i] = -1;
        while (j >= 0 && nums[j] != j) {
          int t = nums[j];
          nums[j] = j;
          j = t;
        }
      }
      
      vector<int> ret;
      for (int i = 0; i < nums.size(); i++) {
        if (nums[i] == -1) ret.push_back(i + 1);
      }
      return ret;
    }
};