class Solution {
public:
    vector<int> topKFrequent(vector<int>& nums, int k) {
      unordered_map<int, int> m;
      for (auto num : nums) {
        if (m.find(num) == m.end()) m[num] = 1;
        else m[num]++;
      }
      
      vector<int> ret;
      for (auto kv : m) {
        ret.push_back(kv.first);
      }
      std::sort(ret.begin(), ret.end(), [&](int a, int b) {
        return m[a] > m[b];
      });
      
      return vector<int>(ret.begin(), ret.begin() + k);
    }
};