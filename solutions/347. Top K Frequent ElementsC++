// Runtime: 20 ms, faster than 90.92% of C++ online submissions for Top K Frequent Elements.
// Memory Usage: 11.6 MB, less than 22.86% of C++ online submissions for Top K Frequent Elements.
​
class Solution {
public:
    vector<int> topKFrequent(vector<int>& nums, int k) {
      unordered_map<int, int> m;
      for (auto num : nums) m[num]++;
      
      vector<int> ret;
      for (auto kv : m) ret.push_back(kv.first);
      nth_element(ret.begin(), ret.begin() + k, ret.end(), 
                 [&m](int a, int b) {
                   return m[a] > m[b];
                 });      
      ret.resize(k);
      return ret;
    }
};
