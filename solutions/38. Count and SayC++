class Solution {
public:
  string countAndSay(int n) {
    string ret = "1";
    for (int i = 2; i <= n; i++) {
      string tmp;
      char ch = ret[0];
      int count = 1; 
      for (int j = 1; j < ret.length(); j++) {
        if (ret[j] == ret[j-1]) {
          count++;
        } else {
          tmp += to_string(count) + string(1, ch);
          count = 1;
          ch = ret[j];
        }
      }
      tmp += to_string(count) + string(1, ch);
      ret = tmp;
    }
    return ret;
  }
};