class Solution {
public:
  vector<int> productExceptSelf(vector<int> &nums) {
    if (nums.size() == 0)
      return nums;
    if (nums.size() == 1)
      return {1};

    vector<int> ret(nums.size());
    ret[nums.size() - 1] = 1;
    for (int i = nums.size() - 2; i >= 0; i--)
      ret[i] = nums[i + 1] * ret[i + 1];

    int t = nums[0];
    nums[0] = 1;
    for (int i = 1; i < nums.size(); i++) {
      int t1 = nums[i];
      nums[i] = nums[i - 1] * t;
      t = t1;
    }

    for (int i = 0; i < nums.size(); i++)
      ret[i] = ret[i] * nums[i];
    return ret;
  }
};