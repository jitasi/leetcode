class Solution {
public:
  char findTheDifference(string s, string t) {
    int m[26];
    for (int i = 0; i < 26; i++)
      m[i] = 0;
    for (int i = 0; i < s.size(); i++)
      m[s[i] - 'a']++;
    for (int i = 0; i < t.size(); i++) {
      m[t[i] - 'a']--;
    }
    for (int i = 0; i < 26; i++)
      if (m[i] < 0)
        return 'a' + i;
  }
};