class Solution {
public:
    bool canJump(vector<int>& nums) {
      int last_can_jump = 0;
      for (int i = 0; i <= last_can_jump; i++) {
        int t = i + nums[i];
        if (last_can_jump < t)
          last_can_jump = t;
        if (last_can_jump >= nums.size() - 1) return true;
      }
      
      return false;
    }
};