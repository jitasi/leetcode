// Runtime: 8 ms, faster than 94.94% of C++ online submissions for Rectangle Area.
// Memory Usage: 8.2 MB, less than 30.94% of C++ online submissions for Rectangle Area.
​
class Solution {
public:
  long overlap(long a1, long b1, long a2, long b2) {
    long t1 = max(b1, b2) - min(a1, a2);
    long t2 = b1 - a1 + b2 - a2;
    if (t1 >= t2) return 0;
    return t2 - t1;
  }
  
  int computeArea(int A, int B, int C, int D, int E, int F, int G, int H) {
    return long(C - A) * (D - B) + long(G - E) * (H - F) - overlap(A, C, E, G) * overlap(B, D, F, H);
  }
};
